{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "rules": {
    "prettier": true,
    "interface-name": false,
    "ordered-imports": [
      true,
      {
        "grouped-imports": true,
        "groups": [{
          "name": "react",
          "match": "react",
          "order": 0
        }]
      }
    ],
    "object-literal-sort-keys": false
  }
}
